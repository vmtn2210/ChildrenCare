@model ChildrenCare.Entities.Prescription

@{
    ViewData["Title"] = "Your Pescription Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <h1>Your Pescription Details</h1>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                Customer Name
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CustomerAccount.FullName)
            </dd>
            <dt class="col-sm-2">
                Staff Name
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.AuthorAccount.FullName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PrescriptionContent)
            </dt>
            <dd class="col-sm-10">
                Prescription Content
            </dd>
            <dt class="col-sm-2">
                Created Date
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CreatedDate)
            </dd>
        </dl>
</div>